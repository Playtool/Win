\ No newline at end of file[m
[32m+[m[32m# --- Notification Optimizations for Windows 11 Pro ---[m
[32m+[m
[32m+[m[32m# 1. Disable toast (banner) notifications[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\PushNotifications" -Name "ToastEnabled" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# 2. Disable notification sounds[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\Notifications\Settings" -Name "NOC_GLOBAL_SETTING_ALLOW_NOTIFICATION_SOUND" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# 3. Disable notifications above lock screen[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\Notifications\Settings" -Name "NOC_GLOBAL_SETTING_ALLOW_TOASTS_ABOVE_LOCK" -Value 0 -Type DWord[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\PushNotifications" -Name "LockScreenToastEnabled" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# 4. Disable critical notifications above lock screen (reminders, VoIP calls)[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\Notifications\Settings" -Name "NOC_GLOBAL_SETTING_ALLOW_CRITICAL_TOASTS_ABOVE_LOCK" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# 5. Disable Security and Maintenance notifications[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\Notifications\Settings\Windows.SystemToast.SecurityAndMaintenance" -Name "Enabled" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# 6. Disable Capability Access notifications[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\Notifications\Settings\Windows.SystemToast.CapabilityAccess" -Name "Enabled" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# 7. Disable Startup App notifications[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\Notifications\Settings\Windows.SystemToast.StartupApp" -Name "Enabled" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# 8. Disable System Setting Engagement notifications[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\SOFTWARE\Microsoft\Windows\CurrentVersion\UserProfileEngagement" -Name "ScoobeSystemSettingEnabled" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# 9. Disable Clock Change notifications (DST changes)[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\Control Panel\Desktop" -Name "DstNotification" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# --- End of Notification Optimizations ---[m
\ No newline at end of file[m
