[32m+[m
[32m+[m[32m# Launch to This PC (1 = This PC, 2 = Quick access)[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced" -Name "LaunchTo" -Value 1 -Type DWord[m
[32m+[m
[32m+[m[32m# Show File Extensions (0 = show, 1 = hide)[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced" -Name "HideFileExt" -Value 0 -Type DWord[m
[32m+[m
[32m+[m[32m# Folder Tips (1 = enabled, 0 = disabled)[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced" -Name "FolderContentsInfoTip" -Value 1 -Type DWord[m
[32m+[m
[32m+[m[32m# Pop-up Descriptions (1 = show, 0 = hide)[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced" -Name "ShowInfoTip" -Value 1 -Type DWord[m
[32m+[m
[32m+[m[32m# Preview Handlers (1 = enabled, 0 = disabled)[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced" -Name "ShowPreviewHandlers" -Value 1 -Type DWord[m
[32m+[m
[32m+[m[32m# Status Bar (1 = show, 0 = hide)[m
[32m+[m[32mSet-ItemProperty -Path "HKCU:\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced" -Name "ShowStatusBar" -Value 1 -Type DWord[m
[32m+[m
